ept </FwbButton>\n        </div>\n      </template>\n    </FwbModal>\n  </div>\n</template>\n\n<script>\nimport { FwbButton, FwbModal } from \"flowbite-vue\";\nexport default {\n  components: {\n    FwbButton,\n    FwbModal,\n  },\n  data() {\n    return {\n      isShowModal: true,\n    };\n  },\n\n  methods: {\n    closeModal() {\n      this.isShowModal = false;\n    },\n    showModal() {\n      this.isShowModal = true;\n    },\n  },\n};\n</script>\n"],"mappings":"AAkCA,SAASA,SAAS,EAAEC,QAAO,QAAS,cAAc;AAClD,eAAe;EACbC,UAAU,EAAE;IACVF,SAAS;IACTC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACF,WAAU,GAAI,KAAK;IAC1B,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,IAAI,CAACH,WAAU,GAAI,IAAI;IACzB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}